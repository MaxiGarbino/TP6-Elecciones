@{
    ViewData["Title"] = "Home Page";
}

     <h1>Elecciones Presidenciales 2023 Argentina</h1>
    @foreach (Partido item in ViewBag.ListaPartidos)
    {
        <div class="index">
        <a href='@Url.Action("VerDetallePartido",new {IDPartido=@item.IDPartido})'><img src="/Fotos/@item.Logo"></a>
        <h3>@item.Nombre</h3>
        </div>
    }
