@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">

<header>
    <h1 class="title">Elecciones Presidenciales 2023 Argentina</h1>
</header>
@foreach (Partido item in ViewBag.ListaPartidos)
{
    <div class="partidos">
        <a href='@Url.Action("VerDetallePartido",new {IDPartido=@item.IDPartido})'><img src="@item.Logo"></a>
        <h3>@item.Nombre</h3>
    </div>
}