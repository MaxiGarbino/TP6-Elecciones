@{
    ViewData["Title"] = "Home Page";
}
<div>
    <section class="imagen">
        
    </section>
    @foreach (Partido item in ViewBag.ListaPartidos)
    {
        <a href='@Url.Action("VerDetallePartido",new {IDPartido=@item.IDPartido})'>
            <div class="partidos">
                <div>
                    <img src="@item.Logo">
                    <h3>@item.Nombre</h3>
                </div>
            </div>
        </a>
    }
</div>